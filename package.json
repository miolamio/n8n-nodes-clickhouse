{
  "name": "n8n-nodes-clickhouse",
  "version": "0.0.1",
  "description": "Query and ingest into your ClickHouse instance",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "author": {
    "name": "Jakub Kaflik",
    "email": "jakub@kaflik.pl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jkaflik/n8n-nodes-clickhouse.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ClickHouseApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ClickHouse/Clickhouse.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^5.0.1",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "@clickhouse/client": "^1.12.0"
  }
}
